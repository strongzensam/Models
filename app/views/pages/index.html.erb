<h1>CONTACTS</h1>
<table border = 1px solid black>

<tr>
<th>First</th>
<th>Middle</th>
<th>Last</th>
<th>Full Name</th>
<th>Email</th>
<th>Phone</th>
<th>Time Created</th>
</tr>
<% @contacts.each do |contact| %>
<tr>
<td><%= link_to contact.first_name, "/contacts/#{contact.id}" %></td>
<td><%= contact.middle_name %></td>
<td><%= contact.last_name %></td>
<td><%= contact.full_name %></td>
<td><%= contact.email %></td>
<td><%= contact.japanese %></td>
<td><%= contact.friendly_created_at %></td>
<td><%= contact.latitude %></td>
<td><%= contact.longitude %></td>
</tr>
<% end -%>

</table>
<h1><%= link_to "Add Contact", "/contacts/new", method: :get %></h1>